<tryton>
    <data grouped="1">
    <record model="ir.message" id="connection_successful">
        <field name="text">Successful IMAP test connection using account "%(account)s".</field>
    </record>
    <record model="ir.message" id="test_details">
        <field name="text">IMAP test connection Details:

%(msg)s</field>
    </record>
    <record model="ir.message" id="connection_error">
        <field name="text">IMAP connection test failed.</field>
    </record>
    <record model="ir.message" id="general_error">
        <field name="text">Error IMAP Server:

%(msg)s</field>
    </record>
    <record model="ir.message" id="login_error">
        <field name="text">Error IMAP Server:
Login with user "%(user)s" failed.

%(msg)s</field>
    </record>
    <record model="ir.message" id="select_error">
        <field name="text">Error IMAP Server:
Could not select "%(folder)s" mailbox.

%(msg)s</field>
    </record>
    <record model="ir.message" id="search_error">
        <field name="text">Error IMAP Server:
Could not search "%(criteria)s" messages.

%(msg)s</field>
    </record>
    <record model="ir.message" id="fetch_error">
        <field name="text">Error IMAP Server:
Could not get id "%(email)s" messages.

%(msg)s</field>
    </record>
    </data>
</tryton>
